<style>

.custom-icon-text-section .col-wrapper {
    text-align: center;
}

.custom-icon-text-section .icon-wrapper {
    display: flex;
    justify-content: center;
    align-items: center;
}

.custom-icon-text-section .svg-content-wrapper {
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
    align-items: center;
}

.custom-icon-text-section .icon-inner {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    display: flex;
}

.custom-icon-text-section {
    margin: 72px 0px;
}

.custom-icon-text-section .icon-text-grid {
  display: flex;
  flex-wrap: wrap;
  gap: 2rem;
  justify-content: space-between;
  max-width: 1200px;
  margin: 0 auto;
}

.custom-icon-text-section .icon-text-item {
  flex: 1 1 20%;
  text-align: center;
  min-width: 180px;
}

.custom-icon-text-section .icon-wrapper {
  display: flex;
  justify-content: center;
  align-items: center;
}

.custom-icon-text-section .icon-wrapper .service-icon {
  width: 70px;
  height: 70px;
}

.custom-icon-text-section .heading {
  font-weight: bold;
  margin-bottom: 0.25rem;
  color: #1A1A1A;
